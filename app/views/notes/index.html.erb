<h1>Listing notes</h1>

<table>
  <tr>
    <th>Notetaker</th>
    <th>Title</th>
    <th>Type</th>
    <th>Pages</th>
    <th>Rating</th>
    <th>Listed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.notetaker_id %></td>
    <td><%= note.title %></td>
    <td><%= note.type %></td>
    <td><%= note.pages %></td>
    <td><%= note.rating %></td>
    <td><%= note.listed %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>
